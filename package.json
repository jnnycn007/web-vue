{
  "name": "web-vue",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "description": "基于 Vue3 + TypeScript + Vite + Less 开发的模板项目",
  "scripts": {
    "dev": "vite",
    "push": "sh scripts/push.sh",
    "tag": "sh scripts/tag.sh",
    "build": "run-s format clean build-only",
    "format": "prettier --write src/",
    "clean": "rimraf ./dist",
    "build:tag": "run-p type-check build-only",
    "type-check": "vue-tsc --build --force",
    "build-only": "vite build && esno ./build/script/postBuild.ts",
    "preview": "pnpm build && vite preview --port 9001",
    "analyze": "vite build --mode analyze",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "prepare": "husky"
  },
  "dependencies": {
    "ant-design-vue": "^4.2.6",
    "axios": "^1.10.0",
    "date-fns": "^4.1.0",
    "lodash-es": "^4.17.21",
    "mockjs": "^1.1.0",
    "pinia": "^3.0.3",
    "qs": "^6.14.0",
    "vue": "^3.5.17",
    "vue-amazing-ui": "2.4.14",
    "vue-router": "^4.5.1"
  },
  "devDependencies": {
    "@tsconfig/node22": "^22.0.2",
    "@types/node": "^24.0.3",
    "@vitejs/plugin-vue": "^5.2.4",
    "@vue/eslint-config-prettier": "^10.2.0",
    "@vue/eslint-config-typescript": "^14.5.1",
    "@vue/tsconfig": "^0.7.0",
    "chalk": "^5.4.1",
    "dotenv": "^16.5.0",
    "eslint": "^9.29.0",
    "eslint-plugin-vue": "^10.2.0",
    "esno": "^4.8.0",
    "fs-extra": "^11.3.0",
    "husky": "^9.1.7",
    "less": "^4.3.0",
    "lint-staged": "^16.1.2",
    "npm-run-all2": "^8.0.4",
    "prettier": "^3.6.0",
    "rimraf": "^6.0.1",
    "rollup-plugin-visualizer": "^6.0.3",
    "terser": "^5.43.1",
    "typescript": "~5.8.3",
    "unplugin-auto-import": "^19.3.0",
    "unplugin-vue-components": "^28.7.0",
    "vite": "^6.3.5",
    "vite-plugin-cdn-import": "^1.0.1",
    "vite-plugin-compression": "^0.5.1",
    "vite-plugin-html": "^3.2.2",
    "vite-plugin-mock": "^3.0.2",
    "vite-plugin-vue-devtools": "^7.7.7",
    "vue-tsc": "^2.2.10"
  },
  "lint-staged": {
    "*.{js,ts}": [
      "prettier --write",
      "eslint --fix"
    ],
    "*.vue": [
      "prettier --parser=vue --write",
      "eslint --fix"
    ],
    "*.{html,css,less,scss}": [
      "prettier --write"
    ],
    "*.md": [
      "prettier --write --parser markdown --prose-wrap never",
      "eslint --fix"
    ]
  }
}
