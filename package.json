{
  "name": "web-vue",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "description": "Vue3 + TypeScript + Vite + Less + VueRouter 开发",
  "scripts": {
    "dev": "vite",
    "push": "sh scripts/push.sh",
    "tag": "sh scripts/tag.sh",
    "build": "run-s format clean build-only",
    "format": "prettier --write src/",
    "clean": "rimraf ./dist",
    "build:tag": "run-p type-check build-only",
    "type-check": "vue-tsc --build --force",
    "build-only": "vite build && esno ./build/script/postBuild.ts",
    "preview": "pnpm build && vite preview --port 9001",
    "analyze": "vite build --mode analyze",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore"
  },
  "dependencies": {
    "ant-design-vue": "^4.2.5",
    "axios": "^1.7.7",
    "date-fns": "^4.1.0",
    "lodash-es": "^4.17.21",
    "pinia": "^2.2.4",
    "qs": "^6.13.0",
    "vue": "^3.5.12",
    "vue-amazing-ui": "^1.8.4",
    "vue-router": "^4.4.5"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.10.4",
    "@tsconfig/node20": "^20.1.4",
    "@types/node": "^22.7.6",
    "@vitejs/plugin-vue": "^5.1.4",
    "@vue/eslint-config-prettier": "^10.0.0",
    "@vue/eslint-config-typescript": "^14.1.1",
    "@vue/tsconfig": "^0.5.1",
    "chalk": "^5.3.0",
    "dotenv": "^16.4.5",
    "eslint": "^9.12.0",
    "eslint-plugin-vue": "^9.29.0",
    "esno": "^4.8.0",
    "fs-extra": "^11.2.0",
    "less": "^4.2.0",
    "npm-run-all2": "^6.2.3",
    "prettier": "^3.3.3",
    "rimraf": "^6.0.1",
    "rollup-plugin-visualizer": "^5.12.0",
    "typescript": "~5.6.3",
    "unplugin-auto-import": "^0.18.3",
    "unplugin-vue-components": "^0.27.4",
    "vite": "^5.4.9",
    "vite-plugin-cdn-import": "^1.0.1",
    "vite-plugin-compression": "^0.5.1",
    "vite-plugin-html": "^3.2.2",
    "vue-tsc": "^2.1.6"
  }
}
