{
  "name": "web-vue",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "author": "theMuseCatcher",
  "description": "基于 Vue3 + TypeScript + Vite + Less 开发的模板项目",
  "scripts": {
    "dev": "vite",
    "push": "sh scripts/push.sh",
    "tag": "sh scripts/tag.sh",
    "build": "run-s format clean build-only",
    "format": "prettier --write src/",
    "clean": "rimraf ./dist",
    "type-check": "vue-tsc --build --force",
    "build-only": "vite build && esno ./build/script/postBuild.ts",
    "preview": "pnpm build && vite preview --port 9001",
    "analyze": "vite build --mode analyze",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "prepare": "husky"
  },
  "dependencies": {
    "@vueuse/core": "^14.0.0",
    "ant-design-vue": "^4.2.6",
    "axios": "^1.13.2",
    "date-fns": "^4.1.0",
    "lodash-es": "^4.17.21",
    "mockjs": "^1.1.0",
    "pinia": "^3.0.4",
    "qs": "^6.14.0",
    "vue": "^3.5.24",
    "vue-amazing-ui": "2.4.23",
    "vue-router": "^4.6.3"
  },
  "devDependencies": {
    "@tsconfig/node22": "^22.0.5",
    "@types/node": "^24.10.1",
    "@vitejs/plugin-vue": "^6.0.1",
    "@vue/eslint-config-prettier": "^10.2.0",
    "@vue/eslint-config-typescript": "^14.6.0",
    "@vue/tsconfig": "^0.8.1",
    "chalk": "^5.6.2",
    "dotenv": "^17.2.3",
    "eslint": "^9.39.1",
    "eslint-plugin-vue": "^10.5.1",
    "esno": "^4.8.0",
    "fs-extra": "^11.3.2",
    "husky": "^9.1.7",
    "less": "^4.4.2",
    "lint-staged": "^16.2.6",
    "npm-run-all2": "^8.0.4",
    "prettier": "^3.6.2",
    "rimraf": "^6.1.0",
    "rollup-plugin-visualizer": "^6.0.5",
    "terser": "^5.44.1",
    "typescript": "~5.9.3",
    "unplugin-auto-import": "^20.2.0",
    "unplugin-vue-components": "^30.0.0",
    "vite": "^7.2.2",
    "vite-plugin-cdn-import": "^1.0.1",
    "vite-plugin-compression": "^0.5.1",
    "vite-plugin-html": "^3.2.2",
    "vite-plugin-mock": "^3.0.2",
    "vite-plugin-vue-devtools": "^8.0.5",
    "vite-svg-loader": "^5.1.0",
    "vue-tsc": "^3.1.4"
  },
  "keywords": [
    "Vue3",
    "TypeScript",
    "Vite",
    "Less",
    "Web",
    "Template",
    "VueAmazingUI",
    "Out-of-the-box"
  ],
  "lint-staged": {
    "*.{js,ts}": [
      "prettier --write",
      "eslint --fix"
    ],
    "*.vue": [
      "prettier --parser=vue --write",
      "eslint --fix"
    ],
    "*.{html,css,less,scss}": [
      "prettier --write"
    ],
    "*.md": [
      "prettier --write --parser markdown --prose-wrap never",
      "eslint --fix"
    ]
  }
}
