{
  "extends": "@vue/tsconfig/tsconfig.dom.json",
  "include": [
    "src/**/*",
    "src/**/*.ts",
    "src/**/*.vue",
    "types/**/*.d.ts",
    "build/*.ts",
    "build/**/*.ts",
    "*.d.ts",
    "vite.config.ts"
  ],
  "exclude": [
    "node_modules",
    "dist"
  ],
  "compilerOptions": {
    "composite": true,
    "tsBuildInfoFile": "./node_modules/.tmp/tsconfig.app.tsbuildinfo",
    "moduleResolution": "node",
    "baseUrl": ".",
    "paths": {
      "@/*": ["src/*"],
      "#/*": ["types/*"],
      "apis/*": ["src/apis/*"],
      "images/*": ["src/assets/images/*"],
      "less/*": ["src/assets/less/*"],
      "components/*": ["src/components/*"],
      "enums/*": ["src/enums/*"],
      "hooks/*": ["src/hooks/*"],
      "http/*": ["src/http/*"],
      "utils/*": ["src/utils/*"]
    }
  }
}
